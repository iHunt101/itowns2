{
  "name": "itowns",
  "version": "0.0.0-alpha",
  "description": "A JS/WebGL framework for 3D geospatial data visualization",
  "main": "dist/itowns.js",
  "scripts": {
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\" \"examples/**/*.js\"",
    "doc": "jsdoc src/Core/Commander/Interfaces/ApiInterface/*",
    "doclint": "npm run doc -- -t templates/silent",
    "test": "npm run lint && npm run build && npm run doclint",
    "build": "webpack -p",
    "start": "NODE_ENV=development webpack-dev-server -d --inline --hot",
    "prepublish": "npm run build"
  },
  "files": [
    "*.md",
    "dist",
    "doc",
    "examples"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iTowns/itowns2.git"
  },
  "license": "(CECILL-B OR MIT)",
  "bugs": {
    "url": "https://github.com/iTowns/itowns2/issues"
  },
  "homepage": "https://itowns.github.io/",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-runtime": "^6.20.0",
    "custom-event": "^1.0.1",
    "es6-promise": "^4.0.5",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "chart.js": "^2.4.0",
    "dat.gui": "^0.6.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "imports-loader": "^0.7.0",
    "js-priority-queue": "^0.1.5",
    "jsdoc": "^3.4.3",
    "jszip": "^3.1.3",
    "paralleljs": "^0.2.1",
    "raw-loader": "^0.5.1",
    "simd": "^2.0.0",
    "string_format": "^0.0.6",
    "three": "^0.84.0",
    "earcut":"^2.1.1",
    "togeojson":"^0.16.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "whatwg-fetch": "^2.0.2"
  }
}
